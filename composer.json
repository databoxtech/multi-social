{
    "name": "databoxtech/multi-social",
    "type": "library",
    "description": "Php library for pushing social posts to multiple social medias simultaneously with minimum coding.",
    "keywords": [
        "databoxtech",
        "multi-social"
    ],
    "homepage": "https://github.com/databoxtech/multi-social",
    "license": "MIT",
    "authors": [
        {
            "name": "Prabath Perera",
            "email": "prabath@databoxtech.com",
            "homepage": "https://www.databoxtech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "facebook/graph-sdk": "^5.7",
        "phpunit/phpunit": "^8.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "databoxtech\\multisocial\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
